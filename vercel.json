{
    "version": 2,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/(.*)", "dest": "app.py"}
    ],
    "headers": [
    {
      "source": "/templates/kdocs.html",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "clipboard-read=*, clipboard-write=*, autofocus=*, midi=*, encrypted-media=*"
        }
      ]
      }
    ]
}