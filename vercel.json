{
    "version": 2,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/(.*)", "dest": "app.py"}
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "clipboard-read=(self), clipboard-write=(self), autofocus=(self), midi=*, encrypted-media=*"
        }
      ]
      }
    ]
}